<div class="container-fluid" style="height: 100%;">
	<div class="row row-eq-height">
		<div class="col-xs-4" style="background-color: #CEE3F9; padding-top: 30px;">
			@{Html.RenderPartial("_WarningLogInfo"); }
		</div>
		<!--/.col-xs-4 -->
		<div class="col-xs-8">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
				<h3>Review</h3>
			</div>

			<div class="modal-body" style="font-size: small;">
				<strong>Coaching Reason(s):</strong>
				<div class="table-responsive">
					<table class="table table-hover">
						@foreach (var reason in Model.LogDetail.Reasons)
						{
							<tr>
								<td>@reason.Description</td>
								<td>
									@foreach (var sub in reason.SubReasons)
									{
										@sub <br />
									}
								</td>
								<td>@reason.Value</td>
							</tr>
						}
					</table>
				</div>
			</div>
			<!-- /.modal-body -->

			<div class="modal-footer">
				<div class="form-group">
					<button class="btn btn-primary float-right" id="btn-close" data-dismiss="modal">Close</button>
				</div>
			</div>
		</div>
		<!--/.col-xs-8 -->
	</div>
	<!--/.row -->
</div>



