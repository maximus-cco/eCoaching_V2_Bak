<div class="row">
	<div class="col-xs-9">
		<div class="panel panel-default">
			@if (Model.LogCountForSiteList.Count > 7)
			{
				<div class="chart-container-big">
					<canvas id="myBar"></canvas>
				</div>
			}
			else if (Model.LogCountForSiteList.Count <4)
			{
				<div class="chart-container-small">
					<canvas id="myBar"></canvas>
				</div>
			}
			else
			{
				<div class="chart-container">
					<canvas id="myBar"></canvas>
				</div>
			}
		</div>
	</div>

	<div class="col-xs-3">
		<div class="panel panel-default">
			@{ Html.RenderPartial("_CountBySite"); }
		</div>
	</div>
</div>

<!-- Display Log List -->
<div id="div-log-list">
</div>

@Scripts.Render("~/bundles/scripts/eclChart")
