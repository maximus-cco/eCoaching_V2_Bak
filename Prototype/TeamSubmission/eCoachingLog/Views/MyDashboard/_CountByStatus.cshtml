<table class="table table-hover">
	<tbody>
		@foreach (var logCount in Model.LogCountList)
		{
			<tr>
				<td><b>@logCount.Description</b></td>
				<td class="text-center"><b>@logCount.Count</b></td>
				@if (logCount.Count > 0)
				{
					<td>
						<a href="#" class="my-dashboard-log-list" data-log="@logCount.LogListPageName">View</a>
					</td>
				}
				else
				{
					<td></td>
				}
			</tr>
		}
	</tbody>
</table>
